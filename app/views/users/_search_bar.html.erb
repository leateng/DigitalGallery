<%= content_for :header do %>
  <%= form_tag users_path, class: "navbar-form navbar-left", method: :get do %>
    <div class="form-group">
      <%= select_tag :app, options_for_select({"已上传APP": :on, "未上传APP": :off}, params[:app]), include_blank: true, class: "form-control header-search-input" %>
    </div>

    <div class="form-group">
      <%= text_field_tag :search, params[:search], class: "form-control header-search-input", placeholder: "姓名 订单号" %>
    </div>

    <%= hidden_field_tag "role", params[:role] %>

    <%= submit_tag "Search", class: "btn btn-default" %>
  <% end %>
<% end %>