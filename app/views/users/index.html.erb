<table class="table">
  <thead>
  <tr>
    <th>id</th>
    <th>头像</th>
    <th>姓名</th>
    <th>邮箱</th>
    <th>电话</th>
    <th>操作</th>
  </tr>
  </thead>

  <tbody>
  <%@users.each do |user|%>
  <tr>
    <td><%=user.id%></td>
    <td><%=link_to gravatar_for(user), gallery_user_path(user)%></td>
    <td><%=user.name%></td>
    <td><%=user.email%></td>
    <td><%=user.telephone%></td>
    <td></td>
  </tr>
  <%end%>
  </tbody>
</table>